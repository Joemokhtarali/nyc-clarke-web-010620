<h1>Drinks</h1>

<div class="index-container">
  <% @drinks.each do |drink| %>
    <div class="index-tile">
      <p><%# link_to drink.name, drink_path(drink.id) %></p>
      <p><%= link_to drink.name, drink %></p>
      <img class="index-img" src="<%= drink.img_url %>" alt="<%= drink.img_url %>">
      <%# link_to drink.name, "/drinks/10"  %>
    </div>
  <% end %>
</div>
